{
  "name": "backend",
  "version": "0.1.0",
  "description": "aris backend",
  "main": "./src/server.ts",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "pre-build": "dotnet ./lbe/LuaBuildEvents.dll ./lbe/script/pre-build.lua",
    "start": "node ./build/bundle.js --config-.config/server.json",
    "dev": "nodemon --exec \"npm run pre-build && ts-node ./src/server.ts --config-.config/server.json\"",
    "dev-ts": "tsnd --ignore-watch node_modules ./src/server.ts --config-.config/server.json",
    "knex:seed": "knex --knexfile knexfile.ts seed:run",
    "knex:migrate": "knex --knexfile knexfile.ts migrate:latest",
    "knex:rollback": "knex --knexfile knexfile.ts migrate:rollback",
    "knex:rollback-all": "knex --knexfile knexfile.ts migrate:rollback --all",
    "test": "jest --verbose --expand --detectOpenHandles --logHeapUsage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SteamsLab/dev_backend.git"
  },
  "author": "Gabriel Nori",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SteamsLab/dev_backend/issues"
  },
  "homepage": "https://github.com/SteamsLab/dev_backend#readme",
  "dependencies": {
    "argon2": "^0.28.2",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jimp": "^0.16.1",
    "joi": "^17.4.0",
    "knex": "^0.95.6",
    "minio": "^7.0.18",
    "mysql2": "^2.2.5",
    "node-schedule": "^2.0.0",
    "nodemailer": "^6.6.1",
    "redis": "^3.1.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@types/bluebird": "^3.5.35",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/minio": "^7.0.7",
    "@types/node-schedule": "^1.3.2",
    "@types/nodemailer": "^6.4.2",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "jest": "^27.0.1",
    "node-mocks-http": "^1.10.1",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.1",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-node-externals": "^3.0.0"
  }
}
