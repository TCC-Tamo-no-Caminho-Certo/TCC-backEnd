{
  "name": "backend",
  "version": "0.1.0",
  "description": "aris backend",
  "main": "./src/server.ts",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "pre-build": "dotnet ./lbe/LuaBuildEvents.dll ./lbe/script/pre-build.lua",
    "start": "node ./build/bundle.js --config-.config/server.json",
    "dev": "nodemon --exec \"npm run pre-build && ts-node ./src/server.ts --config-.config/server.json\"",
    "dev-ts": "tsnd --ignore-watch node_modules ./src/server.ts --config-.config/server.json",
    "knex:seed": "knex --knexfile knexfile.ts seed:run",
    "knex:migrate": "knex --knexfile knexfile.ts migrate:latest",
    "knex:rollback": "knex --knexfile knexfile.ts migrate:rollback",
    "knex:rollback-all": "knex --knexfile knexfile.ts migrate:rollback --all",
    "test": "jest --verbose --expand --detectOpenHandles --logHeapUsage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SteamsLab/dev_backend.git"
  },
  "author": "Gabriel Nori",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SteamsLab/dev_backend/issues"
  },
  "homepage": "https://github.com/SteamsLab/dev_backend#readme",
  "dependencies": {
    "@types/minio": "^7.0.7",
    "argon2": "^0.26.2",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "jimp": "^0.16.1",
    "joi": "^17.3.0",
    "knex": "^0.21.15",
    "minio": "^7.0.18",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.4.17",
    "redis": "^3.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@types/bluebird": "^3.5.33",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.10",
    "@types/helmet": "0.0.47",
    "@types/jest": "^26.0.20",
    "@types/joi": "^14.3.4",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "node-mocks-http": "^1.10.0",
    "nodemon": "^2.0.7",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.1.1",
    "typescript": "^3.9.7",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  }
}
