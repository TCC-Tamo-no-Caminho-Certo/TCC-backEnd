{
  "name": "backend",
  "version": "0.1.0",
  "description": "aris backend",
  "main": "./src/server.js",
  "scripts": {
    "build": "tsc",
    "pre-build": "dotnet ./lbe/LuaBuildEvents.dll ./lbe/script/pre-build.lua",
    "start": "node ./build/server.js",
    "dev": "nodemon --exec \"npm run pre-build && ts-node ./src/server.ts\"",
    "dev-ts": "tsnd --ignore-watch node_modules ./src/server.ts",
    "knex:seed": "knex --knexfile knexfile.ts seed:run",
    "knex:migrate": "knex --knexfile knexfile.ts migrate:latest",
    "knex:rollback": "knex --knexfile knexfile.ts migrate:rollback",
    "knex:rollback-all": "knex --knexfile knexfile.ts migrate:rollback --all",
    "test": "jest --runInBand --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SteamsLab/dev_backend.git"
  },
  "author": "Gabriel Nori",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SteamsLab/dev_backend/issues"
  },
  "homepage": "https://github.com/SteamsLab/dev_backend#readme",
  "dependencies": {
    "argon2": "^0.26.2",
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "joi": "^17.1.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.2",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.10",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/helmet": "0.0.47",
    "@types/jest": "^26.0.7",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.25",
    "@types/supertest": "^2.0.10",
    "dotenv": "^8.2.0",
    "jest": "^25.5.4",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  }
}
